{
    "name": "node-mysql-signup-verification-api",
    "version": "1.0.0",
    "description": "NodeJS + MySQL API for Email Sign Up with Verification, Authentication & Forgot Password",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/cornflourblue/node-mysql-signup-verification-api.git"
    },
    "scripts": {
        "start": "node ./server.js",
        "start:dev": "nodemon ./server.js",
        "db:create": "npx sequelize-cli db:create",
        "db:migrate": "npx sequelize-cli db:migrate",
        "db:migrate:fresh": "npx sequelize-cli db:migrate:undo && npx sequelize-cli db:migrate",
        "db:seed": "npx sequelize-cli db:seed:all",
        "db:reset": "npx sequelize-cli db:drop && npx sequelize-cli db:create && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all"
    },
    "dependencies": {
        "all": "^0.0.0",
        "bcryptjs": "^2.4.3",
        "body-parser": "^1.19.0",
        "cookie-parser": "^1.4.5",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.17.1",
        "express-handlebars": "^7.1.2",
        "express-jwt": "^6.0.0",
        "fs": "^0.0.1-security",
        "joi": "^17.2.1",
        "jsonwebtoken": "^8.5.1",
        "mysql2": "^2.2.5",
        "nodemailer": "^6.4.11",
        "nodemailer-express-handlebars": "^6.1.0",
        "rootpath": "^0.1.2",
        "sequelize": "^6.35.2",
        "swagger-ui-express": "^4.1.4",
        "yamljs": "^0.3.0"
    },
    "devDependencies": {
        "nodemon": "^2.0.3",
        "sequelize-cli": "^6.6.2"
    }
}
